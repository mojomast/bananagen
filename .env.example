# Bananagen Environment Configuration
# Copy this file to .env and fill in your actual API keys

# Primary AI Providers
# Choose one or more providers based on your needs

# Gemini API Configuration (Google AI Studio)
GEMINI_API_KEY=your_gemini_api_key_here

# OpenRouter API Configuration (Multi-model access)
OPENROUTER_API_KEY=your_openrouter_api_key_here
OPENROUTER_MODEL=google/gemini-2.5-flash-image-preview
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1

# General Configuration
BANANAGEN_LOG_LEVEL=INFO
BANANAGEN_DEFAULT_PROVIDER=gemini
BANANAGEN_DEFAULT_WIDTH=512
BANANAGEN_DEFAULT_HEIGHT=512
